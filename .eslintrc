{
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "semi": ["error", "always"],
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": ["eslint:recommended"],
}
